{
    "simple": {
        "syntax": {
            "properties": {
                "test": "<'property'>",
                "property": "foo"
            }
        },
        "valid": [
            "foo"
        ],
        "invalid": [
            "",
            "bar"
        ]
    },
    "with multiplier": {
        "syntax": {
            "properties": {
                "test": "<'property'>+",
                "property": "foo"
            }
        },
        "valid": [
            "foo",
            "foo foo",
            "foo foo foo foo"
        ],
        "invalid": [
            "foo foo bar",
            ""
        ]
    }
}
