{
    "simple": {
        "syntax": {
            "types": {
                "bar": "foo"
            },
            "properties": {
                "test": "<bar>"
            }
        },
        "valid": [
            "foo"
        ],
        "invalid": [
            "",
            "bar"
        ]
    },
    "with multiplier": {
        "syntax": {
            "types": {
                "bar": "foo"
            },
            "properties": {
                "test": "<bar>+"
            }
        },
        "valid": [
            "foo",
            "foo foo",
            "foo foo foo foo"
        ],
        "invalid": [
            "foo foo bar",
            ""
        ]
    }
}
